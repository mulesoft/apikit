#%RAML 1.0
---
title: Simple API
baseUri: http://www.google.com
types:
  Person:
    type: object
    properties:
      name:
        required: true
        type: string
/single-resource:
  get:
    responses:
      200:
        body:
          application/json: !!null
          text/xml: !!null
/api:
  /sub-resource:
    get:
      responses:
        200:
          body:
            application/json: !!null
            text/xml: !!null
  /sub-resource-types:
    post:
      body:
        application/json:
          type: Person
      responses:
        200: ~