{
  "name": "xml-viewer",
  "version": "0.1.2",
  "license": "Apache-2.0 OR CC-BY-4.0",
  "description": "An XML payload viewer for the XML response",
  "devDependencies": {
    "polymer-cli": "^0.16.0"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "private": false,
  "main": [
    "xml-viewer.html"
  ],
  "author": "The Advanced REST client authors <arc@mulesoft.com>",
  "scripts": {
    "serve": "node_modules/.bin/polymer serve",
    "lint": "node_modules/.bin/polymer lint xml-viewer.html",
    "test-sauce": "node_modules/.bin/polymer test --plugin sauce --job-name \"xml-viewer:local-test\"",
    "test": "node_modules/.bin/polymer test",
    "deps": "npm install --quiet && bower install --quiet",
    "update-deps": "npm update --quiet && bower update --quiet"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/advanced-rest-client/xml-viewer.git"
  },
  "bugs": {
    "url": "https://github.com/advanced-rest-client/xml-viewer/issues",
    "email": "arc@mulesoft.com"
  }
}