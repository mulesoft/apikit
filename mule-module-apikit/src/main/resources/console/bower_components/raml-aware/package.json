{
  "name": "raml-aware",
  "version": "1.0.3",
  "license": "Apache-2.0 OR CC-BY-4.0",
  "description": "Element that is aware of the RAML content",
  "devDependencies": {
    "polymer-cli": "^0.17.0"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "private": false,
  "main": [
    "raml-aware.html"
  ],
  "author": "The Advanced REST client authors",
  "scripts": {
    "serve": "node_modules/.bin/polymer serve",
    "lint": "node_modules/.bin/polymer lint raml-aware.html",
    "test-sauce": "node_modules/.bin/polymer test --plugin sauce --job-name \"raml-aware:local-test\"",
    "test": "node_modules/.bin/polymer test",
    "deps": "npm install --quiet && bower install --quiet",
    "update-deps": "npm update --quiet && bower update --quiet"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/advanced-rest-client/raml-aware.git"
  }
}