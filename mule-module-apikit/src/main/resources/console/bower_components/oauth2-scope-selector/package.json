{
  "name": "oauth2-scope-selector",
  "version": "1.0.6",
  "license": "Apache-2.0 OR CC-BY-4.0",
  "description": "A selector for the OAuth scope. It provides an UI to enter a scope for the AOuth settings",
  "devDependencies": {
    "polymer-cli": "^0.17.0"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "private": false,
  "main": [
    "oauth2-scope-selector.html"
  ],
  "author": "The Advanced REST client authors <arc@mulesoft.com>",
  "scripts": {
    "serve": "node_modules/.bin/polymer serve",
    "lint": "node_modules/.bin/polymer lint oauth2-scope-selector.html",
    "test-sauce": "node_modules/.bin/polymer test --plugin sauce --job-name \"oauth2-scope-selector:local-test\"",
    "test": "exit 0",
    "deps": "npm install --quiet && bower install --quiet",
    "update-deps": "npm update --quiet && bower update --quiet"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/advanced-rest-client/oauth2-scope-selector.git"
  },
  "bugs": {
    "url": "https://github.com/advanced-rest-client/oauth2-scope-selector/issues",
    "email": "arc@mulesoft.com"
  }
}