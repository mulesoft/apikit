{
  "name": "oauth-authorization",
  "version": "0.1.7",
  "license": "Apache-2.0 OR CC-BY-4.0",
  "description": "A set of elements that perform oauth authorization",
  "devDependencies": {
    "polymer-cli": "^0.17.0"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "private": false,
  "main": [
    "outh-authorization.html"
  ],
  "author": "The Advanced REST client authors <arc@mulesoft.com>",
  "scripts": {
    "serve": "node_modules/.bin/polymer serve",
    "lint": "node_modules/.bin/polymer lint oauth-authorization.html",
    "test-sauce": "node_modules/.bin/polymer test --plugin sauce --job-name \"oauth-authorization:local-test\"",
    "test": "node_modules/.bin/polymer test",
    "deps": "npm install --quiet && bower install --quiet",
    "update-deps": "npm update --quiet && bower update --quiet"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/advanced-rest-client/oauth-authorization.git"
