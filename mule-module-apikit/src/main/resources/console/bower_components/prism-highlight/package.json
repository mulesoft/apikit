{
  "name": "prism-highlight",
  "version": "2.0.2",
  "license": "Apache-2.0 OR CC-BY-4.0",
  "description": "Syntax highlighting via Prism",
  "devDependencies": {
    "polymer-cli": "^0.17.0"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "private": false,
  "main": [
    "prism-highlight.html"
  ],
  "author": "The Advanced REST Client authors",
  "scripts": {
    "serve": "node_modules/.bin/polymer serve",
    "lint": "node_modules/.bin/polymer lint prism-highlight.html",
    "test-sauce": "node_modules/.bin/polymer test --plugin sauce --job-name \"prism-highlight:local-test\"",
    "test": "node_modules/.bin/polymer test",
    "deps": "npm install --quiet && bower install --quiet",
    "update-deps": "npm update --quiet && bower update --quiet"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/advanced-rest-client/prism-highlight.git"
  }
}