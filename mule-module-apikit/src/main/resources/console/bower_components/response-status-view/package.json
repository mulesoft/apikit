{
  "name": "response-status-view",
  "version": "0.1.8",
  "license": "Apache-2.0 OR CC-BY-4.0",
  "description": "HTTP response status view, including status, headers redirects and timings",
  "devDependencies": {
    "polymer-cli": "^0.16.0"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "private": false,
  "main": [
    "response-status-view.html"
  ],
  "author": "The Advanced REST client authors <arc@mulesoft.com>",
  "scripts": {
    "serve": "node_modules/.bin/polymer serve",
    "lint": "node_modules/.bin/polymer lint response-status-view.html",
    "test-sauce": "exit 0; node_modules/.bin/polymer test --plugin sauce --job-name \"response-status-view:local-test\"",
    "test": "exit 0; node_modules/.bin/polymer test",
    "deps": "npm install --quiet && bower install --quiet",
    "update-deps": "npm update --quiet && bower update --quiet"
